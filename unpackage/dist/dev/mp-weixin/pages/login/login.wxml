<view class="login_box"><image class="bg-img" mode="scaleToFill" src="../../static/img/news/login_bg.png"></image><view class="main"><image class="logo-img" src="{{logoObj.logo}}"></image><view class="title">{{logoObj.name}}</view><view class="acount-box"><text class="account-title">账号：</text><input class="ipt-tel" placeholder="请输入注册手机号" type="number" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['userInfo']]]]]}}" value="{{userInfo.username}}" bindinput="__e"/></view><view class="acount-box passwd"><text class="account-title">密码：</text><input class="ipt-tel " password placeholder="初始密码为身份证后六位" type="text" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userInfo']]]]]}}" value="{{userInfo.password}}" bindinput="__e"/></view><button class="btn-login" type="primary" data-event-opts="{{[['tap',[['loginJump',['$event']]]]]}}" bindtap="__e">登录</button><view data-event-opts="{{[['tap',[['jumpRegster',['$event']]]]]}}" class="jump-reg" bindtap="__e">注册账号</view></view></view>