page {
  position: relative;
  width: 100%;
  height: 100%;
}
.title-box {
  font-size: 34rpx;
  color: #000000;
  width: 750rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  font-weight: 500;
}
.content {
  font-size: 28rpx;
  line-height: 48rpx;
  padding: 30rpx;
}
.isFlxed {
  position: fixed;
  bottom: 4rpx;
  left: 30rpx;
}
.btn-login {
  margin: 140rpx auto 44rpx;
  color: #ffffff;
  font-size: 36rpx;
  width: 690rpx;
  height: 94rpx;
  background: #2256d8 !important;
  border-radius: 10rpx;
}
.fan-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  padding: 30rpx 30rpx 10rpx 30rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #8d8d8d;
  font-size: 28rpx;
}
.fan-content {
  padding: 30rpx 30rpx 0 30rpx;
  color: #8d8d8d;
  font-size: 28rpx;
  line-height: 40rpx;
}
/* 提示窗口 */
.uni-tip {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  width: 540rpx;
  height: 424rpx;
  background-color: #fff;
  border-radius: 14rpx;
}
.uni-tip-title {
  margin-top: 27rpx;
  margin-bottom: 45rpx;
  text-align: center;
  font-size: 36rpx;
  color: #000;
}
.popup_textarea {
  width: 430rpx;
  height: 195rpx;
  font-size: 28rpx;
  margin-left: 55rpx;
  border: 1px #f1f1f1 solid;
  margin-bottom: -15rpx;
}
.uni-tip-content {
  /* padding: 15px;
	 */
  font-size: 30rpx;
  color: #888888;
  text-align: center;
}
.uni-tip-group-button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  text-align: center;
  margin-top: 40rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #f1f1f1;
}
.uni-tip-button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  font-size: 36rpx;
  color: #4399fc;
}
.uni-tip-button1 {
  color: #0a0a0a;
  border-right: 1rpx solid #f1f1f1;
}

